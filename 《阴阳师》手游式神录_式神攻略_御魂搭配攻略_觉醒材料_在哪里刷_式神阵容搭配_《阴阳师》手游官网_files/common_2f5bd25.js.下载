nie.define(function(){function o(){e.removeClass("show"),d.removeClass("hover"),$(".nav_hover").removeClass("nav_hover")}function s(s){return s?(e.addClass("show"),d.addClass("hover"),void e.find('.topbar_more_list[data-tab="'+s+'"]').addClass("show").siblings(".topbar_more_list").removeClass("show")):void o()}function i(){$(window).width()<1870?$(".topnav_list .xxhd").addClass("hide"):$(".topnav_list .xxhd").removeClass("hide")}function a(o){$(window).width()>=1650&&o>=55?$(".topnav_list .ssl,.topnav_list .zjl,.topnav_list .xxhd").removeClass("hide"):($(".topnav_list .ssl,.topnav_list .zjl").addClass("hide"),$(window).width()<1870&&$(".topnav_list .xxhd").addClass("hide"))}$("#top_bar").show(),$(window).scroll(function(){var o=$(window).scrollTop(),s=$("#top_bar"),i=$(".main_banner .logo span");o>=55?(s.addClass("fixed"),i.addClass("hide")):(s.removeClass("fixed"),i.removeClass("hide")),a(o)});var t=null,n=$("#top_bar .topnav_list a"),e=$(".topbar_more "),d=$("#top_bar");n.mouseenter(function(){t&&clearTimeout(t);var o=$(this).data("tab");$(this).addClass("nav_hover").parent("li").siblings().find("a").removeClass("nav_hover"),s(o)}),n.mouseleave(function(){t=setTimeout(function(){o()},200)}),e.mouseenter(function(){t&&clearTimeout(t)}),e.mouseleave(function(){t=setTimeout(function(){o()},200)}),$(".topnav_list li").width($(window).width()<1440?"110px":"114px"),$(window).resize(function(){$(".topnav_list li").width($(window).width()<1440?"110px":"114px"),i(),a($(window).scrollTop())}),i()});